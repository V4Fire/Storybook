{
  "name": "@v4fire/storybook-framework-webpack5",
  "version": "0.1.0",
  "description": "Storybook framework for the V4Fire with webpack5 builder",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Storybook",
    "V4Fire",
    "webpack5"
  ],
  "homepage": "https://github.com/V4Fire/Storybook/tree/main/packages/frameworks/v4fire-webpack5",
  "bugs": {
    "url": "https://github.com/V4Fire/Storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/V4Fire/Storybook.git",
    "directory": "packages/frameworks/v4fire-webpack5"
  },
  "author": "",
  "license": "MIT",
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preset": {
      "require": "./dist/preset.js",
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "template/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "dependencies": {
    "@storybook/addons": "7.0.18",
    "@storybook/builder-webpack5": "7.0.18",
    "@storybook/core-common": "7.0.18",
    "@storybook/node-logger": "7.0.18"
  },
  "devDependencies": {
    "@v4fire/client": "^4.0.0-beta.4",
    "typescript": "4.6.2",
    "webpack": "5.82.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^7.0.0",
    "@v4fire/client": "^4.0.0-beta.4",
    "webpack": "^5.82.1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
